        "painting_names = []#НАЗВАНИЯ КАРТИН создает пустую строку при переходе к новому автору\n",
        "painting_author = []\n",
        "painting_years = []\n",
        "painting_sizes = []\n",
        "painting_styles = []\n",
        "painting_disctiption = []\n",
        "painting_genres = []\n",
        "painting_materials = []\n",
        "painting_techniques = []\n",
        "painting_galleries = []\n",
        "painting_cities = []\n",
        "painting_countries = []\n",
        "\n",
        "for link in paintings_links:\n",
        "  url = link\n",
        "  page_descr = requests.get(url, headers={'User-Agent': UserAgent().chrome})\n",
        "  soup3 = BeautifulSoup(page_descr.text, \"html.parser\")\n",
        "\n",
        "  title_str = soup3.title.text\n",
        "  name = re.findall(r'\"([^\"]*)\"', title_str)\n",
        "  painting_names.append(''.join(name))\n",
        "\n",
        "  author = re.findall(r'.*\\w+(?=\\s\")', title_str) \n",
        "  painting_author.append(''.join(author))\n",
        "\n",
        "  discription_text = soup3.findAll('div', class_='entry-content')[0].text\n",
        "  description = re.findall(r'[^\\n\\r].*(?=Год написания картины)', discription_text, re.DOTALL)\n",
        "  painting_disctiption.append(''.join(description))\n",
        "\n",
        "  page_text =  soup3.article.text\n",
        "  year = re.findall(r'картины: (\\d{4})+', page_text)\n",
        "  painting_years.append(''.join(map(str,year)))\n",
        "  \n",
        "  size = re.findall(r'\\d{2,4}.*\\sx\\s\\d{2,4}.*', page_text)\n",
        "  painting_sizes.append(''.join(map(str,size)))\n",
        "\n",
        "  style = re.findall(r'Стиль:\\s(\\w+)', page_text)\n",
        "  painting_styles.append(''.join(style))\n",
        "\n",
        "  genre = re.findall(r'Жанр:\\s(\\w+)', page_text)\n",
        "  painting_genres.append(''.join(genre))\n",
        "\n",
        "  material = re.findall(r'Материал:\\s(\\w+)', page_text)\n",
        "  painting_materials.append(''.join(material))\n",
        "\n",
        "  technique = re.findall(r'Техника написания:\\s(\\w+)', page_text)\n",
        "  painting_techniques.append(''.join(technique))\n",
        "\n",
        "  gallery = re.findall(r'Галерея:\\s(.*(?=,\\s.*,\\s.*))', page_text)\n",
        "  painting_galleries.append(''.join(gallery))\n",
        "\n",
        "  city = re.findall(r'Галерея:\\s.*,(.*(?=,\\s\\w+))', page_text)\n",
        "  painting_cities.append(''.join(city))\n",
        "\n",
        "  country = re.findall(r'Галерея:\\s.*,.*,\\s(.*(?=\\.))', page_text)\n",
        "  painting_countries.append(''.join(country))\n",
        "\n",
        "  dict_art = {'painting':painting_names,'author':painting_author,'year':painting_years, 'size':painting_sizes,\n",
        "              'material':painting_materials, 'techniqe':painting_techniques, 'genre':painting_genres, 'style':painting_styles,\n",
        "              'disctiption':painting_disctiption, 'gallery':painting_galleries, 'location':painting_cities, 'country':painting_countries}\n",
        "\n",
        "  df = pd.DataFrame(data = dict_art)\n",
        "  df.to_csv('art_data.csv')"
